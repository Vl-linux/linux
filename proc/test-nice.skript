#! /bin/bash

#echo Процесс копирования 100 ГБ нулей в никуда...
#dd if=/dev/zero of=/dev/null bs=100M count=1000
#echo -----------------------------------------
echo Выполняется процесс копирования с высоким приоритетом...
nice -n -20 dd if=/dev/zero of=/dev/null bs=100M count=1000
echo ------------------------------------------
echo Выполняется процесс копирования с низким приоритетом...
nice -n 19 dd if=/dev/zero of=/dev/null bs=100M count=1000
echo Выполнение всех процесов завершено
exit 0
